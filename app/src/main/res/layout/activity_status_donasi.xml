<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    android:id="@+id/main"
    tools:context=".DonationDetailActivity">

    <!-- Top Bar -->
    <com.google.android.material.card.MaterialCardView
        android:id="@+id/topBarCard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardElevation="4dp"
        app:cardCornerRadius="0dp"
        app:strokeWidth="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/topBar"
            android:layout_width="match_parent"
            android:layout_height="64dp"
            android:background="@color/green_primary">

            <ImageView
                android:id="@+id/backButton"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:src="@drawable/ic_arrow_back"
                android:contentDescription="Tombol Kembali"
                android:layout_marginStart="16dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

            <TextView
                android:id="@+id/pageTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Detail Donasi"
                android:textSize="16sp"
                android:fontFamily="@font/poppins_bold"
                android:textColor="@color/black"
                android:textFontWeight="700"
                app:layout_constraintStart_toEndOf="@id/backButton"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginStart="16dp"/>

        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/topBarCard"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:fillViewport="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="16dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Hari Ini"
                android:textSize="14sp"
                android:textColor="#000000"
                android:fontFamily="@font/poppins_regular"
                android:layout_marginBottom="8dp"/>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/cardDonasi1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="16dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="16dp"
                app:cardElevation="4dp"
                app:strokeWidth="0dp">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="16dp">

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/guidelineVerticalKiri"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintGuide_begin="80dp" />

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/guidelineVerticalTengah"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintGuide_percent="0.5"/>

                    <ImageView
                        android:id="@+id/iconDonasi1"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:src="@drawable/ic_box"
                        android:contentDescription="Ikon Donasi"
                        android:background="@drawable/ic_notification_bg"
                        android:padding="4dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"/>

                    <TextView
                        android:id="@+id/namaPanti1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="Panti Bina Harapan"
                        android:textSize="14sp"
                        android:textColor="#000000"
                        android:fontFamily="@font/poppins_bold"
                        android:textFontWeight="700"
                        android:layout_marginStart="12dp"
                        app:layout_constraintTop_toTopOf="@id/iconDonasi1"
                        app:layout_constraintStart_toEndOf="@id/iconDonasi1"
                        app:layout_constraintEnd_toEndOf="parent"/>

                    <TextView
                        android:id="@+id/idDonasi1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="W2702330052025"
                        android:textSize="12sp"
                        android:textColor="@color/grey_font"
                        android:fontFamily="@font/poppins_regular"
                        android:layout_marginStart="12dp"
                        android:layout_marginTop="0dp"
                        app:layout_constraintTop_toBottomOf="@id/namaPanti1"
                        app:layout_constraintStart_toEndOf="@id/iconDonasi1"
                        app:layout_constraintEnd_toEndOf="parent"/>

                    <!-- Bagian Kiri (Dari, Ke) -->
                    <TextView
                        android:id="@+id/labelDari1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Dari :"
                        android:textSize="10sp"
                        android:textColor="@color/grey_font"
                        android:fontFamily="@font/poppins_regular"
                        android:layout_marginTop="8dp"
                        app:layout_constraintTop_toBottomOf="@id/idDonasi1"
                        app:layout_constraintStart_toStartOf="parent"/>

                    <TextView
                        android:id="@+id/nilaiDari1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Sentul Point"
                        android:textSize="12sp"
                        android:textColor="#000000"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_marginTop="2dp"
                        app:layout_constraintTop_toBottomOf="@id/labelDari1"
                        app:layout_constraintStart_toStartOf="@id/labelDari1"/>

                    <TextView
                        android:id="@+id/labelKe1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Ke :"
                        android:textSize="10sp"
                        android:textColor="@color/grey_font"
                        android:fontFamily="@font/poppins_regular"
                        android:layout_marginTop="4dp"
                        app:layout_constraintTop_toBottomOf="@id/nilaiDari1"
                        app:layout_constraintStart_toStartOf="parent"/>

                    <TextView
                        android:id="@+id/nilaiKe1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Panti Bina Harapan"
                        android:textSize="12sp"
                        android:textColor="#000000"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_marginTop="2dp"
                        app:layout_constraintTop_toBottomOf="@id/labelKe1"
                        app:layout_constraintStart_toStartOf="@id/labelKe1"/>

                    <!-- Bagian Kanan (Status, Detail) -->
                    <TextView
                        android:id="@+id/labelStatus1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Status :"
                        android:textSize="10sp"
                        android:textColor="@color/grey_font"
                        android:fontFamily="@font/poppins_regular"
                        app:layout_constraintTop_toTopOf="@id/labelDari1"
                        app:layout_constraintStart_toStartOf="@id/guidelineVerticalTengah"/>

                    <TextView
                        android:id="@+id/nilaiStatus1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Pengiriman"
                        android:textSize="12sp"
                        android:textColor="@color/black"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_marginTop="2dp"
                        app:layout_constraintTop_toBottomOf="@id/labelStatus1"
                        app:layout_constraintStart_toStartOf="@id/labelStatus1"/>

                    <TextView
                        android:id="@+id/labelDetail1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Detail :"
                        android:textSize="10sp"
                        android:textColor="@color/grey_font"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_marginTop="4dp"
                        app:layout_constraintTop_toBottomOf="@id/nilaiStatus1"
                        app:layout_constraintStart_toStartOf="@id/guidelineVerticalTengah"/>

                    <TextView
                        android:id="@+id/nilaiDetail1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Bantuan Pangan"
                        android:textSize="12sp"
                        android:textColor="#000000"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_marginTop="2dp"
                        app:layout_constraintTop_toBottomOf="@id/labelDetail1"
                        app:layout_constraintStart_toStartOf="@id/labelDetail1"/>

                    <Button
                        android:id="@+id/lihatDetailBarang"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Lihat Detailnya"
                        android:textSize="12sp"
                        android:fontFamily="@font/poppins_bold"
                        android:textFontWeight="700"
                        android:backgroundTint="@color/green_primary"
                        android:textColor="@color/black"
                        android:paddingStart="16dp"
                        android:paddingEnd="16dp"
                        android:minHeight="0dp"
                        android:paddingTop="8dp"
                        android:paddingBottom="8dp"
                        app:cornerRadius="8dp"
                        android:layout_marginTop="16dp"
                        app:layout_constraintTop_toBottomOf="@id/nilaiDetail1"
                        app:layout_constraintEnd_toEndOf="parent"/>

                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>

            <!-- Kartu Donasi 2 (Sekolah Jebre) -->
            <com.google.android.material.card.MaterialCardView
                android:id="@+id/cardDonasi2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="16dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="16dp"
                app:cardElevation="4dp"
                app:strokeWidth="0dp">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="16dp">

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/guidelineVertical2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintGuide_begin="80dp" />

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/guidelineVerticalTengah2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintGuide_percent="0.5"/>

                    <ImageView
                        android:id="@+id/iconDonasi2"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:src="@drawable/ic_transaksi_uang"
                        android:contentDescription="Ikon Donasi"
                        android:background="@drawable/ic_notification_bg"
                        android:padding="4dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"/>

                    <TextView
                        android:id="@+id/namaSekolah2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="Sekolah Jebre"
                        android:textSize="14sp"
                        android:textColor="#000000"
                        android:fontFamily="@font/poppins_bold"
                        android:textFontWeight="700"
                        android:layout_marginStart="12dp"
                        app:layout_constraintTop_toTopOf="@id/iconDonasi2"
                        app:layout_constraintStart_toEndOf="@id/iconDonasi2"
                        app:layout_constraintEnd_toEndOf="parent"/>

                    <TextView
                        android:id="@+id/idDonasi2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="TR2702330052025"
                        android:textSize="12sp"
                        android:textColor="@color/grey_font"
                        android:fontFamily="@font/poppins_regular"
                        android:layout_marginStart="12dp"
                        android:layout_marginTop="0dp"
                        app:layout_constraintTop_toBottomOf="@id/namaSekolah2"
                        app:layout_constraintStart_toEndOf="@id/iconDonasi2"
                        app:layout_constraintEnd_toEndOf="parent"/>

                    <TextView
                        android:id="@+id/labelSumberDana2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Sumber Dana :"
                        android:textSize="10sp"
                        android:textColor="@color/grey_font"
                        android:fontFamily="@font/poppins_regular"
                        android:layout_marginTop="8dp"
                        app:layout_constraintTop_toBottomOf="@id/idDonasi2"
                        app:layout_constraintStart_toStartOf="parent"/>

                    <TextView
                        android:id="@+id/nilaiSumberDana2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Saldo Rahmat"
                        android:textSize="12sp"
                        android:textColor="#000000"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_marginTop="2dp"
                        app:layout_constraintTop_toBottomOf="@id/labelSumberDana2"
                        app:layout_constraintStart_toStartOf="@id/labelSumberDana2"/>

                    <TextView
                        android:id="@+id/labelTujuan2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Tujuan :"
                        android:textSize="10sp"
                        android:textColor="@color/grey_font"
                        android:fontFamily="@font/poppins_regular"
                        android:layout_marginTop="4dp"
                        app:layout_constraintTop_toBottomOf="@id/nilaiSumberDana2"
                        app:layout_constraintStart_toStartOf="parent"/>

                    <TextView
                        android:id="@+id/nilaiTujuan2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Sekolah Jebre"
                        android:textSize="12sp"
                        android:textColor="#000000"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_marginTop="2dp"
                        app:layout_constraintTop_toBottomOf="@id/labelTujuan2"
                        app:layout_constraintStart_toStartOf="@id/labelTujuan2"/>

                    <TextView
                        android:id="@+id/labelStatus2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Status :"
                        android:textSize="10sp"
                        android:textColor="@color/grey_font"
                        android:fontFamily="@font/poppins_regular"
                        app:layout_constraintTop_toTopOf="@id/labelSumberDana2"
                        app:layout_constraintStart_toStartOf="@id/guidelineVerticalTengah2"/>

                    <TextView
                        android:id="@+id/nilaiStatus2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Dana Disalurkan"
                        android:textSize="12sp"
                        android:textColor="@color/black"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_marginTop="2dp"
                        app:layout_constraintTop_toBottomOf="@id/labelStatus2"
                        app:layout_constraintStart_toStartOf="@id/labelStatus2"/>

                    <TextView
                        android:id="@+id/labelDetail2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Detail :"
                        android:textSize="10sp"
                        android:textColor="@color/grey_font"
                        android:fontFamily="@font/poppins_regular"
                        android:layout_marginTop="4dp"
                        app:layout_constraintTop_toBottomOf="@id/nilaiStatus2"
                        app:layout_constraintStart_toStartOf="@id/guidelineVerticalTengah2"/>

                    <TextView
                        android:id="@+id/nilaiDetail2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Biaya Pendidikan"
                        android:textSize="12sp"
                        android:textColor="#000000"
                        android:fontFamily="@font/poppins_medium"
                        android:layout_marginTop="2dp"
                        app:layout_constraintTop_toBottomOf="@id/labelDetail2"
                        app:layout_constraintStart_toStartOf="@id/labelDetail2"/>

                    <Button
                        android:id="@+id/lihatDetailUang"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Lihat Detailnya"
                        android:textSize="12sp"
                        android:fontFamily="@font/poppins_bold"
                        android:textFontWeight="700"
                        android:backgroundTint="@color/green_primary"
                        android:textColor="@color/black"
                        android:paddingStart="16dp"
                        android:paddingEnd="16dp"
                        android:minHeight="0dp"
                        android:paddingTop="8dp"
                        android:paddingBottom="8dp"
                        app:cornerRadius="8dp"
                        android:layout_marginTop="16dp"
                        app:layout_constraintTop_toBottomOf="@id/nilaiDetail2"
                        app:layout_constraintEnd_toEndOf="parent"/>

                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>

